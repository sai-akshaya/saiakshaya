		<script src=C:\Users\Smile\Downloads\jquery-1.11.0.min.js></script>
		<script>

		var no_swap=0;
		var no_updown=0;
		var no_memory=0;
		var no_resize=0;
			
			
		$(document).ready(function() {
			$("#swap").click(function()
			{var $bbox=$("#bbox");
			var $rbox=$("#rbox");
			var $lbox=$("#lbox");
			
			if(no_resize%2===0)
			{
				if(no_swap%2==0)
					{
					$rbox.animate({left:"-200px"},"slow");
					$lbox.animate({left:"410px"},"slow");
					}
				else
					{
					$rbox.animate({left:"8px"},"slow");
					$lbox.animate({left:"0px"},"slow");
					}
			}
			else
				{
				if(no_swap%2==0)
					{
					$rbox.animate({left:"-200px"},"slow");
					$lbox.animate({left:"410px"},"slow");
					$bbox.animate({left:"410px"},"slow");

					}
				else
					{
					$rbox.animate({left:"10px"},"slow");
					$lbox.animate({left:"0px"},"slow");
					$bbox.animate({left:"0px"},"slow");
					}
				}
			no_swap++;
		}
			
		);
		
			$("#updown").click(function()
			{var $bbox=$("#bbox");
			var $rbox=$("#rbox");
			var $lbox=$("#lbox");
			
			if(no_resize%2===0)
				{
				if(no_updown%2==0)
					{
					$rbox.animate({top:"200px"},"slow");
					$lbox.animate({top:"200px"},"slow");
					$bbox.animate({top:"0px"},"slow");
					}
				else
					{
					$rbox.animate({top:"0px"},"slow");
					$lbox.animate({top:"0px"},"slow");
					$bbox.animate({top:"310px"},"slow");
					}
			}
			else
				{
				if(no_updown%2==0)
					{
					$lbox.animate({top:"200px"},"slow");
					$bbox.animate({top:"0px"},"slow");

					}
				else
					{
					$lbox.animate({top:"0px"},"slow");
					$bbox.animate({top:"310px"},"slow");
					}
				}
			no_updown++;
		
			
		});});
		</script>
